        <TextBox x:Name="tbName" HorizontalAlignment="Left" Margin="265,43,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="313" Height="25"/>
        <Label x:Name="lblName" Content="Name" HorizontalAlignment="Left" Margin="37,43,0,0" VerticalAlignment="Top" Width="152" Height="25"/>
